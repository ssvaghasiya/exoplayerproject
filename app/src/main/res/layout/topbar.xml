<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

        <import type="android.view.View" />

        <variable
            name="isBackShow"
            type="java.lang.Boolean" />


        <variable
            name="isSettingShow"
            type="java.lang.Boolean" />


        <variable
            name="isLayoutShow"
            type="java.lang.Boolean" />

        <variable
            name="isTextShow"
            type="java.lang.Boolean" />

        <variable
            name="isEditShow"
            type="java.lang.Boolean" />

        <variable
            name="isNotificationShow"
            type="java.lang.Boolean" />

        <variable
            name="isCenterTextShow"
            type="java.lang.Boolean" />

        <variable
            name="isAddFriendShow"
            type="java.lang.Boolean" />

        <variable
            name="isBackRoundedShow"
            type="java.lang.Boolean" />

        <variable
            name="isStartTextShow"
            type="java.lang.Boolean" />

        <variable
            name="isStartText1Show"
            type="java.lang.Boolean" />

        <variable
            name="isHeadPhonesShow"
            type="java.lang.Boolean" />

        <variable
            name="isShareShow"
            type="java.lang.Boolean" />

        <variable
            name="isSearchShow"
            type="java.lang.Boolean" />

        <variable
            name="isLogoShow"
            type="java.lang.Boolean" />

        <variable
            name="isNavShow"
            type="java.lang.Boolean" />


        <variable
            name="topBarClickListener"
            type="com.newproject.interfaces.TopBarClickListener" />


    </data>



    <com.google.android.material.appbar.AppBarLayout
        android:id="@+id/appBarLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@android:color/holo_red_light"
        android:theme="@style/AppTheme.AppBarOverlay"
        app:elevation="0dp"
        android:fitsSystemWindows="true"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:background="@android:color/transparent"
            app:layout_collapseMode="pin"
            app:layout_scrollFlags="scroll|enterAlways"
            app:popupTheme="@style/Theme.AppCompat.Light.DarkActionBar"
            app:theme="@style/ToolbarTheme">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/dp5"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_gravity="center_vertical|start"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

<!--                    <androidx.appcompat.widget.AppCompatImageView-->
<!--                        android:id="@+id/header_nav_logo"-->
<!--                        android:layout_width="wrap_content"-->
<!--                        android:layout_height="wrap_content"-->
<!--                        android:src="@drawable/ic_navigation"-->
<!--                        android:onClick="@{(v)->topBarClickListener.onTopBarClickListener(v,@string/menu)}"-->
<!--                        android:visibility="@{isNavShow? View.VISIBLE : View.GONE, default=visible}" />-->

                    <com.newproject.custom.CTextViewB
                        android:id="@+id/tvTitleText"
                        style="@style/txt_000_25"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginStart="@dimen/dp5"
                        android:singleLine="true"
                        android:text="@string/app_name"
                        android:visibility="@{isTextShow? View.VISIBLE : View.GONE, default=visible}" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentEnd="true"
                    android:layout_centerVertical="true"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <com.newproject.custom.CTextView
                        android:id="@+id/txtCenterText"
                        style="@style/txt_000_25"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:singleLine="true"
                        android:text="@string/app_name"
                        android:visibility="@{isCenterTextShow? View.VISIBLE : View.GONE, default=gone}" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentEnd="true"
                    android:layout_centerVertical="true"
                    android:layout_gravity="center"
                    android:gravity="start"
                    android:orientation="horizontal"
                    android:paddingStart="@dimen/_5sdp">

                    <com.newproject.custom.CTextView
                        android:id="@+id/txtStartText"
                        style="@style/txt_000_25"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:singleLine="true"
                        android:text="@string/app_name"
                        android:visibility="@{isStartTextShow? View.VISIBLE : View.GONE, default=gone}" />
                </LinearLayout>

                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/imgBack"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_gravity="center_vertical|start"
                    android:contentDescription="@null"
                    android:visibility="@{isBackShow? View.VISIBLE : View.GONE, default=gone}" />

                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/imgBackRounded"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_gravity="center_vertical|start"
                    android:contentDescription="@null"
                    android:visibility="@{isBackRoundedShow? View.VISIBLE : View.GONE, default=gone}" />


                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_centerVertical="true"
                    android:layout_marginEnd="@dimen/dp5"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <androidx.appcompat.widget.AppCompatImageView
                        android:id="@+id/imgSearch"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_5sdp"
                        android:contentDescription="@null"
                        android:padding="@dimen/dp5"
                        android:visibility="@{isSearchShow? View.VISIBLE : View.GONE, default=visible}" />

                    <androidx.appcompat.widget.AppCompatImageView
                        android:id="@+id/imgAddFriend"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:contentDescription="@null"
                        android:paddingRight="@dimen/dp5"
                        android:visibility="@{isAddFriendShow? View.VISIBLE : View.GONE, default=visible}" />

                    <androidx.appcompat.widget.AppCompatImageView
                        android:id="@+id/imgNoti"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:contentDescription="@null"
                        android:padding="@dimen/dp5"
                        android:visibility="@{isNotificationShow? View.VISIBLE : View.GONE, default=gone}" />

                    <androidx.appcompat.widget.AppCompatImageView
                        android:id="@+id/imgShare"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:contentDescription="@null"
                        android:padding="@dimen/dp5"
                        android:visibility="@{isShareShow? View.VISIBLE : View.GONE, default=gone}" />
                </LinearLayout>

            </RelativeLayout>
        </androidx.appcompat.widget.Toolbar>
    </com.google.android.material.appbar.AppBarLayout>

